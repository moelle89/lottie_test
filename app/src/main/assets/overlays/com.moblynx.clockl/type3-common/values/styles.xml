<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DeskClockParentTheme" parent="@*android:style/Theme.DeviceDefault.Settings">
        <item name="android:buttonStyle">?android:borderlessButtonStyle</item>
        <item name="android:checkboxStyle">@*com.moblynx.clockl:style/CheckBoxTheme</item>
    </style>
     <style name="DeskClock" parent="@*com.moblynx.clockl:style/DeskClockParentTheme">
        <item name="android:windowBackground">@*android:color/legacy_primary</item>        
		<item name="android:colorBackground">@*android:color/dim_foreground_light</item>
        <item name="android:dropDownListViewStyle">@*com.moblynx.clockl:style/DeskClockDropDownListView</item>
        <item name="android:actionBarStyle">@*com.moblynx.clockl:style/DeskClockBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:actionBarTabStyle">@*com.moblynx.clockl:style/DeskClockTabStyle</item>
        <item name="android:actionBarTabBarStyle">@*com.moblynx.clockl:style/DeskClockTabBarStyle</item>
    </style>
</resources>