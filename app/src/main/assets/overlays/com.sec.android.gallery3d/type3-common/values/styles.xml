<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Theme.DeviceDefault" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:tabWidgetStyle">@*com.sec.android.gallery3d:style/GalleryTabWidgetStyle</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionBarStyle">@*com.sec.android.gallery3d:style/GalleryActionBar</item>
        <item name="android:actionMenuTextColor">@*com.sec.android.gallery3d:color/actionbar_menu_text_selector</item>
        <item name="android:colorPrimary">@*com.sec.android.gallery3d:color/gallery_color_primary</item>
		<item name="android:backgroundTint">@*android:color/legacy_primary</item>
		<item name="android:windowShowWallpaper">true</item>
		<item name="android:windowIsTranslucent">false</item>		
		<item name="android:windowIsFloating">false</item>
    </style>
    <style name="Theme.Gallery" parent="@*com.sec.android.gallery3d:style/Theme.DeviceDefault">
        <item name="android:displayOptions">none</item>
        <item name="android:actionModeCloseDrawable">@null</item>
        <item name="android:actionModeCloseButtonStyle">@*com.sec.android.gallery3d:style/HideActionModeClose</item>
        <item name="android:alertDialogTheme">@*com.sec.android.gallery3d:style/CustomAlertDialogTheme</item>
        <item name="android:homeAsUpIndicator">@*com.sec.android.gallery3d:drawable/tw_ic_ab_back_material</item>
        <item name="android:colorPrimaryDark">@*com.sec.android.gallery3d:color/gallery_color_primary_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/holo_button_pressed</item>
		<item name="android:backgroundTint">@null</item>
		<item name="android:windowBackground">@*android:color/legacy_primary</item>
		<item name="android:windowShowWallpaper">true</item>
		<item name="android:windowIsTranslucent">false</item>
		<item name="android:windowIsFloating">false</item>
    </style>
