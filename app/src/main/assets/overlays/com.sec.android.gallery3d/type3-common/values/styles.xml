    <style name="Theme.GalleryPhoneWithTitle" parent="@*com.sec.android.gallery3d:style/Theme.Gallery">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">false</item>
        <item name="android:actionMenuTextColor">@*com.sec.android.gallery3d:color/actionbar_menu_text_selector</item>
        <item name="android:statusBarColor">@*com.sec.android.gallery3d:color/gallery_status_bar_background_color</item>
