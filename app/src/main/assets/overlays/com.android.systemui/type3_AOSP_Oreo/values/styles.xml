<?xml version="1.0" encoding="utf-8"?>
<resources>  
    <style name="Theme.SystemUI.Dialog" parent="@android:style/Theme.Material.Dialog">
		<item name="android:colorControlHighlight">@*android:color/holo_button_pressed</item>
	</style>
    <style name="Theme.SystemUI.Dialog.Alert" parent="@android:style/Theme.Material.Dialog.Alert" >
		<item name="android:colorControlHighlight">@*android:color/holo_button_pressed</item>
	</style>
    <style name="DotStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="Widget.TextView.NumPadKey" parent="@android:style/Widget.TextView">
        <item name="android:textSize">32.5sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:paddingBottom">-16.0dip</item>
        <item name="android:singleLine">true</item>
		<item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="Widget.TextView.NumPadKey.Klondike" parent="@*com.android.systemui:style/Widget.TextView.NumPadKey">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/accent_material_dark</item>
        <item name="android:paddingBottom">0.0dip</item>
		<item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.QS" parent="@*com.android.systemui:style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.QS.Subhead" parent="@*com.android.systemui:style/TextAppearance.QS">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.QS.TileLabel" parent="@*com.android.systemui:style/TextAppearance.QS">
        <item name="android:textSize">@*com.android.systemui:dimen/qs_tile_text_size</item>
		<item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
		<item name="android:textColor">@*android:color/white</item>
    </style>
	<!-- qs panel -> add tiles page -->
    <style name="edit_theme" parent="@*android:style/Theme.DeviceDefault.QuickSettings">
        <item name="android:colorBackground">@*android:color/legacy_primary</item>
        <item name="android:colorSecondary">@*android:color/user_icon_7</item>
		<item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
	<!--
    <style name="QSIconTheme">
        <item name="*com.android.systemui:backgroundColor">#8BB3D1E3</item>
        <item name="*com.android.systemui:fillColor">@*com.android.systemui:color/abc_search_url_text_selected</item>
        <item name="*com.android.systemui:singleToneColor">@*com.android.systemui:color/abc_search_url_text_selected</item>
    </style>-->
	<style name="qs_theme" parent="@*android:style/Theme.Material">
        <item name="*com.android.systemui:darkIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="*com.android.systemui:lightIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="android:colorPrimary">@*android:color/legacy_primary</item><!--power menu bg AOSP 8.1 -->
        <item name="android:colorPrimaryDark">@*android:color/legacy_primary</item><!-- advanced power menu bg AOSP 8.1 -->
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:textColorPrimary">#ffffff</item><!-- active tiles AOSP 8.1-->
        <item name="android:colorForeground">@*android:color/lighter_gray</item><!-- battery saver tile, edit icon, settings icon AOSP 8.1-->
        <item name="android:textColorTertiary">#ffffff</item><!-- power menu text AOSP 8.1-->
        <item name="android:colorControlNormal">@*android:color/lighter_gray</item><!-- signal tile letter, expand icon AOSP 8.1-->
    </style>
    <!-- QuickSettings - Power Menu -->
    <style name="qs_base" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:colorPrimary">@*android:color/legacy_primary</item>           <!-- Power Menu - Main -->
        <item name="android:colorPrimaryDark">@*android:color/legacy_primary</item>       <!-- Power Menu - Additional (Bug Report) -->
        <item name="android:colorSecondary">@*android:color/legacy_primary</item>         <!-- QuickSettings - Edit Background -->
        <item name="android:colorBackgroundFloating">@*android:color/legacy_primary</item><!-- QuickSettings, Volume Background -->
        <item name="android:panelColorBackground">@*android:color/legacy_primary</item>   <!-- P - Volume Panel Bottom (Settings) -->
    </style>
</resources>
