<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.NotificationInfo.Header" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
		<item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="TextAppearance.NotificationInfo.Radio" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
		<item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.Volume" parent="@*com.android.systemui:style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.Volume.Header" parent="@*com.android.systemui:style/TextAppearance.Volume">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="keyguard_carrier_label_style">
        <item name="android:textSize">14.0dip</item>
		<item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sec-roboto-condensed</item>
    </style>
	<style name="TextAppearance.StatusBar.Clock" parent="@android:style/TextAppearance.StatusBar.Icon">
	    <item name="android:textSize">14.0dip</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@*com.android.systemui:color/light_mode_icon_color_single_tone</item>
        <item name="android:fontFamily">sec-roboto-condensed</item>
	</style>
	<style name="TextAppearance.StatusBar.Expanded.Clock" parent="@*com.android.systemui:style/TextAppearance.StatusBar.Expanded">
		<item name="android:textSize">15.0dip</item>
        <item name="android:textColor">@*com.android.systemui:color/qs_header_clock</item>
		<item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="TextAppearance.StatusBar.Expanded.Date" parent="@*com.android.systemui:style/TextAppearance.StatusBar.Expanded">
		<item name="android:textSize">@*com.android.systemui:dimen/qs_time_expanded_size</item>
        <item name="android:textColor">@*com.android.systemui:color/qs_header_date</item>
		<item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sec-roboto-condensed</item>
    </style>
    <style name="TextAppearance.NotificationSnooze" parent="@*android:style/TextAppearance" />
    <style name="TextAppearance.NotificationSnooze.Guide" parent="@*com.android.systemui:style/TextAppearance.NotificationSnooze">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
	
    <style name="TextAppearance.NotificationInfo" parent="@*com.android.systemui:style/TextAppearance">
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="hybrid_notification_text">
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:paddingEnd">4.0dip</item>
    </style>
    <style name="hybrid_notification_title">
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:paddingEnd">4.0dip</item>
    </style>	
    <style name="TextAppearance.NotificationInfo.Primary" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:alpha">0.87</item>
    </style>
    <style name="TextAppearance.NotificationInfo.Secondary" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
        <item name="android:alpha">0.54</item>
    </style>
	<style name="TextAppearance.NotificationInfo.Button" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/accent_material_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@*com.android.systemui:drawable/btn_borderless_rect</item>
        <item name="android:focusable">true</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:paddingStart">8.0dip</item>
        <item name="android:paddingEnd">8.0dip</item>
    </style>
    <style name="TextAppearance.NotificationInfo.Secondary.Link" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo.Secondary">
        <item name="android:textColor">@*android:color/accent_material_dark</item>
    </style>
    <style name="Widget.AppCompat.NotificationActionText" parent="">
        <item name="android:textAppearance">?*android:textAppearanceButton</item>
        <item name="android:textSize">@*com.android.systemui:dimen/notification_action_text_size</item>
        <item name="android:textColor">@*android:color/notification_primary_text_color_dark</item>
    </style>

</resources>
