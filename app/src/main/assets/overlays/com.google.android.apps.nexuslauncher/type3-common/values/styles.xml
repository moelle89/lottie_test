<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LauncherThemeDark" parent="@*com.google.android.apps.nexuslauncher:style/LauncherTheme">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorHint">#a0ffffff</item>
        <item name="android:textColorTertiary">#ccffffff</item>
        <item name="android:colorControlHighlight">#a0ffffff</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsInterimScrimAlpha">105</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsNavBarScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:isMainColorDark">true</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorPrimary">@*android:color/dim_foreground_light</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorSecondary">@*android:color/timepicker_default_background_material</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorTertiary">@*android:color/button_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:widgetsTheme">@*com.google.android.apps.nexuslauncher:style/WidgetContainerTheme.Dark</item>
    </style>
    <style name="LauncherTheme" parent="@*com.google.android.apps.nexuslauncher:style/BaseLauncherThemeWithCustomAttrs">
		<item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorHint">#a0ffffff</item>
        <item name="android:textColorTertiary">#ccffffff</item>
        <item name="android:colorControlHighlight">#a0ffffff</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsInterimScrimAlpha">105</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsNavBarScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:isMainColorDark">true</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorPrimary">@*android:color/dim_foreground_light</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorSecondary">@*android:color/timepicker_default_background_material</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorTertiary">@*android:color/button_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:widgetsTheme">@*com.google.android.apps.nexuslauncher:style/WidgetContainerTheme.Dark</item>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#00000000</item>
        <item name="android:navigationBarColor">#00000000</item>
    </style>
   <style name="BaseLauncherThemeWithCustomAttrs" parent="@*com.google.android.apps.nexuslauncher:style/BaseLauncherTheme">
        <item name="*com.google.android.apps.nexuslauncher:allAppsInterimScrimAlpha">105</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsNavBarScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:allAppsScrimColor">@*android:color/secondary_text_default_material_light</item>
        <item name="*com.google.android.apps.nexuslauncher:isMainColorDark">true</item>
        <item name="*com.google.android.apps.nexuslauncher:isWorkspaceDarkText">false</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorPrimary">@*android:color/dim_foreground_light</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorSecondary">@*android:color/timepicker_default_background_material</item>
        <item name="*com.google.android.apps.nexuslauncher:popupColorTertiary">@*android:color/button_material_dark</item>
        <item name="*com.google.android.apps.nexuslauncher:widgetsTheme">@*com.google.android.apps.nexuslauncher:style/WidgetContainerTheme.Dark</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceAmbientShadowColor">#33000000</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceKeyShadowColor">#44000000</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceShadowColor">#b0000000</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceStatusBarScrim">@*com.google.android.apps.nexuslauncher:drawable/workspace_bg</item>
        <item name="*com.google.android.apps.nexuslauncher:workspaceTextColor">@android:color/white</item>
    </style>
</resources>