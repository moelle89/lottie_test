<?xml version="1.0" encoding="utf-8"?>
<layer-list android:paddingMode="stack"
  xmlns:android="http://schemas.android.com/apk/res/android">
<item>
	<shape android:shape="rectangle">
		<solid android:color="@*android:color/dim_foreground_light" />
		<corners android:topLeftRadius="0.0dp" android:topRightRadius="0.0dp"
				 android:bottomLeftRadius="4.5dp" android:bottomRightRadius="4.5dp"/>
	</shape>
</item>
<item>
	<shape android:shape="rectangle" android:tint="@*android:color/accessibility_focus_highlight" >
		<gradient android:startColor="#17000000" android:endColor="#06000000" android:angle="90.0"  />
		<corners android:topLeftRadius="0.0dp" android:topRightRadius="0.0dp"
				 android:bottomLeftRadius="4.5dp" android:bottomRightRadius="4.5dp"/>
	</shape>
</item>
</layer-list>