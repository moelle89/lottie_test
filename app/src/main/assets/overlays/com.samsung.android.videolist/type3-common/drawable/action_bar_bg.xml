<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>
    <item>
        <shape android:tint="#15b3d1e3" android:shape="rectangle">
            <gradient android:startColor="@android:color/black" android:endColor="#35000000" android:angle="-90.0" />
        </shape>
    </item>
    <item android:gravity="top" android:height="1.0dip">
        <shape android:shape="rectangle">
            <solid android:color="#23b3d1e3" />
        </shape>
    </item>
</layer-list>