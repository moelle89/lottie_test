<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Widget.AppCompat.ProgressBar" parent="@*android:style/Widget.Material.ProgressBar" />
    <style name="Base.Widget.AppCompat.ProgressBar.Horizontal" parent="@*android:style/Widget.Material.ProgressBar.Horizontal" />
    <style name="AttachPickerLabel">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="FullScreenDialog" parent="@*com.whatsapp:style/Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:actionModeBackground">@*com.whatsapp:color/action_mode</item>
        <item name="android:actionModeCloseDrawable">@*com.whatsapp:drawable/ic_emoji_search_back</item>
        <item name="android:actionModeCutDrawable">@*com.whatsapp:drawable/abc_ic_menu_cut_mtrl_alpha</item>
        <item name="android:actionModeCopyDrawable">@*com.whatsapp:drawable/abc_ic_menu_copy_mtrl_am_alpha</item>
        <item name="android:actionModePasteDrawable">@*com.whatsapp:drawable/abc_ic_menu_paste_mtrl_am_alpha</item>
        <item name="android:actionModeSelectAllDrawable">@*com.whatsapp:drawable/abc_ic_menu_selectall_mtrl_alpha</item>
        <item name="android:actionModeShareDrawable">@*com.whatsapp:drawable/abc_ic_menu_share_mtrl_alpha</item>
    </style>
    <style name="Theme.App" parent="@*com.whatsapp:style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
		<item name="*com.whatsapp:windowActionModeOverlay">true</item>
    </style>
    <style name="Theme.App.GalleryPicker" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
        <item name="*com.whatsapp:actionOverflowButtonStyle">@*com.whatsapp:style/OverFlowLight</item>
        <item name="*com.whatsapp:actionBarStyle">@*com.whatsapp:style/Widget.Styled.ActionBar.GalleryPicker</item>
        <item name="*com.whatsapp:actionModeBackground">@*android:color/background_floating_material_dark</item>
        <item name="*com.whatsapp:homeAsUpIndicator">@*com.whatsapp:drawable/ic_back_teal</item>
        <item name="*com.whatsapp:searchViewStyle">@*com.whatsapp:style/GalleryPickerSearchView</item>
        <item name="*com.whatsapp:colorPrimary">@*android:color/background_dark</item>
        <item name="*com.whatsapp:colorPrimaryDark">@*android:color/transparent</item>
        <item name="*com.whatsapp:colorAccent">@*com.whatsapp:color/accent</item>
    </style>
    <style name="Widget.Styled.ActionBar" parent="@*com.whatsapp:style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:titleTextStyle">@*com.whatsapp:style/Theme.ActionBar.TitleTextStyle</item>
        <item name="android:subtitleTextStyle">@*com.whatsapp:style/Theme.ActionBar.SubtitleTextStyle</item>
        <item name="android:elevation">@*com.whatsapp:dimen/actionbar_elevation</item>
        <item name="*com.whatsapp:titleTextStyle">@*com.whatsapp:style/Theme.ActionBar.TitleTextStyle</item>
        <item name="*com.whatsapp:subtitleTextStyle">@*com.whatsapp:style/Theme.ActionBar.SubtitleTextStyle</item>
        <item name="*com.whatsapp:elevation">@*com.whatsapp:dimen/actionbar_elevation</item>
    </style>
    <style name="Theme.NoActionBar" parent="@*com.whatsapp:style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>
    <style name="Theme.App.GalleryPicker.NoActionBar" parent="@*com.whatsapp:style/Theme.App.GalleryPicker">
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
    </style>
    <style name="Theme.App.Gray" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>
    <style name="Theme.App.White" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="*com.whatsapp:homeAsUpIndicator">@*com.whatsapp:drawable/ic_back_teal</item>
    </style>
    <style name="Platform.ThemeOverlay.AppCompat" parent="">
        <item name="android:colorControlNormal">@*android:color/white</item>
        <item name="android:colorControlActivated">@*com.whatsapp:color/accent</item>
        <item name="android:colorButtonNormal">@*android:color/button_material_dark</item>
        <item name="android:colorControlHighlight">@*com.whatsapp:color/accent</item>
        <item name="android:colorPrimary">@*com.whatsapp:color/accent</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*com.whatsapp:color/accent</item>
    </style>
    <style name="Base.V21.Theme.AppCompat.Dialog" parent="@*com.whatsapp:style/Base.V7.Theme.AppCompat.Dialog">
        <item name="android:windowElevation">@*com.whatsapp:dimen/abc_floating_window_z</item>
    </style>
    <style name="Platform.AppCompat" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorLink">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?android:buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="android:textColorLinkInverse">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorLink">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?android:buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="android:textColorLinkInverse">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>

	<style name="Base.TextAppearance.AppCompat.Medium" parent="@android:style/TextAppearance.Material.Medium">
		<item name="*com.whatsapp:pstsDividerColor">@android:color/transparent</item>
		<item name="*com.whatsapp:pstsIndicatorColor">@*com.whatsapp:color/tab_indicator</item>
		<item name="*com.whatsapp:pstsIndicatorHeight">3.0dip</item>
		<item name="*com.whatsapp:pstsShouldExpand">true</item>
		<item name="*com.whatsapp:pstsTabPaddingLeftRight">8.0dip</item>
		<item name="*com.whatsapp:pstsUnderlineHeight">1.0dip</item>
		<item name="*com.whatsapp:theme">@*com.whatsapp:style/ThemeOverlay.AppCompat.Light</item>
		<item name="*com.whatsapp:popupTheme">@*com.whatsapp:style/Toolbar</item>
	</style>
    <style name="Toolbar" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorPrimary">@*android:color/white</item>
		<item name="*com.whatsapp:actionModeBackground">@*com.whatsapp:drawable/notification_bg_low_pressed</item>
        <item name="android:textAppearanceMedium">@*com.whatsapp:style/TextAppearanceMenuList</item>
        <item name="*com.whatsapp:colorControlNormal">@*android:color/lighter_gray</item>
        <item name="*com.whatsapp:colorControlActivated">@*android:color/accent_material_dark</item>
		<item name="*com.whatsapp:colorBackgroundFloating">@*android:color/dim_foreground_light</item>
		<item name="*android:windowBackground">@*android:color/background_dark</item>		
		<item name="*android:colorBackground">@*android:color/background_dark</item>
    </style>
</resources>
