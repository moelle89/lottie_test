<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:id="@*com.whatsapp:id/content"
        android:layout_width="262.0dip"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40.0dip"
        android:orientation="vertical">

        <FrameLayout
            android:id="@*com.whatsapp:id/quoted_message_holder"
            style="@*com.whatsapp:style/QuotedFrame"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <FrameLayout
            android:id="@*com.whatsapp:id/map_frame"
            android:layout_width="fill_parent"
            android:layout_height="147.0dip"
            android:layout_centerVertical="true"
            android:padding="@*com.whatsapp:dimen/conversation_video_thumb_padding">

            <ImageView
                android:id="@*com.whatsapp:id/thumb"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:scaleType="centerCrop" />

            <com.whatsapp.location.WaMapView
                android:id="@*com.whatsapp:id/map_holder"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" />

            <Button
                android:id="@*com.whatsapp:id/thumb_button"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@*com.whatsapp:drawable/selector_orange_gradient" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@*com.whatsapp:drawable/balloon_outgoing_frame" />

            <FrameLayout
                android:id="@*com.whatsapp:id/control_frame"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@*com.whatsapp:drawable/download_background">

                <Button
                    android:id="@*com.whatsapp:id/control_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:drawableLeft="@*com.whatsapp:drawable/btn_upload"
                    android:drawablePadding="8.0dip"
                    android:ellipsize="none"
                    android:paddingLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:singleLine="true"
                    android:stateListAnimator="@null"
                    android:textColor="@*com.whatsapp:color/conversation_row_image_text" />

                <ProgressBar
                    android:id="@*com.whatsapp:id/progress_bar"
                    android:layout_width="48.0dip"
                    android:layout_height="48.0dip"
                    android:layout_gravity="center"
                    android:visibility="gone" />
            </FrameLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@*com.whatsapp:id/message_info_holder"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="2.0dip">

            <com.whatsapp.TextEmojiLabel
                android:id="@*com.whatsapp:id/place_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:paddingLeft="8.0dip"
                android:paddingRight="8.0dip"
                android:singleLine="true"
                android:textColor="#ffffffff"
                android:textSize="@*com.whatsapp:dimen/conversation_location_name"
                android:visibility="gone" />

            <TextView
                android:id="@*com.whatsapp:id/place_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:gravity="left"
                android:maxLines="2"
                android:paddingLeft="8.0dip"
                android:paddingRight="8.0dip"
                android:textColor="#99ffffff"
                android:textSize="@*com.whatsapp:dimen/conversation_location_address"
                android:visibility="gone" />

            <TextView
                android:id="@*com.whatsapp:id/host_view"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:ellipsize="start"
                android:maxLines="1"
                android:paddingLeft="8.0dip"
                android:paddingRight="8.0dip"
                android:textColor="#55ffffff"
                android:textSize="@*com.whatsapp:dimen/conversation_location_host"
                android:visibility="gone" />

            <LinearLayout
                android:id="@*com.whatsapp:id/date_wrapper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="3.0dip"
                android:paddingRight="7.0dip">

                <TextView
                    android:id="@*com.whatsapp:id/date"
                    style="@*com.whatsapp:style/ConversationDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:id="@*com.whatsapp:id/status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="4.0dip" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@*com.whatsapp:id/forward"
        style="@*com.whatsapp:style/ForwardButton" />
</merge>