<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.GooglePay.NoActionBar" parent="@*com.google.android.apps.walletnfcrel:style/Platform.AppCompat">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.google.android.apps.walletnfcrel:bottomSheetDialogTheme">@*com.google.android.apps.walletnfcrel:style/BottomSheetDialogTheme.Light</item>
        <item name="*com.google.android.apps.walletnfcrel:colorAccent">@*com.google.android.apps.walletnfcrel:color/google_blue600</item>
        <item name="*com.google.android.apps.walletnfcrel:colorPrimary">@*com.google.android.apps.walletnfcrel:color/google_white</item>
        <item name="*com.google.android.apps.walletnfcrel:materialButtonStyle">@*com.google.android.apps.walletnfcrel:style/Widget.GoogleMaterial.Button</item>
        <item name="*com.google.android.apps.walletnfcrel:popupMenuStyle">@*com.google.android.apps.walletnfcrel:style/Widget.AppCompat.PopupMenu.Overflow</item>
        <item name="*com.google.android.apps.walletnfcrel:selectableItemBackground">@*com.google.android.apps.walletnfcrel:drawable/bounded_rippleable</item>
        <item name="*com.google.android.apps.walletnfcrel:selectableItemBackgroundBorderless">@*com.google.android.apps.walletnfcrel:drawable/unbounded_rippleable</item>
        <item name="*com.google.android.apps.walletnfcrel:textInputStyle">@*com.google.android.apps.walletnfcrel:style/Widget.Design.TextInputLayout</item>
        <item name="*com.google.android.apps.walletnfcrel:viewInflaterClass">com.google.commerce.tapandpay.android.infrastructure.lifecycle.TapAndPayViewInflater</item>
		<item name="android:windowLightNavigationBar">false</item>
		<item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.GooglePay.NoActionBar.LaunchScreen" parent="@*com.google.android.apps.walletnfcrel:style/Platform.V21.AppCompat">
        <item name="android:windowBackground">@*com.google.android.apps.walletnfcrel:drawable/app_launch_screen</item>
		<item name="android:backgroundTint">@null</item>
		<item name="android:windowLightNavigationBar">false</item>
		<item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Widget.GoogleMaterial.BottomNavigationView" parent="@*com.google.android.apps.walletnfcrel:style/Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@*com.google.android.apps.walletnfcrel:drawable/settings_divider</item>
		<item name="android:backgroundTint">@null</item>
        <item name="*com.google.android.apps.walletnfcrel:enforceMaterialTheme">true</item>
        <item name="*com.google.android.apps.walletnfcrel:itemIconTint">@*com.google.android.apps.walletnfcrel:color/google_bottom_nav_icon_tint</item>
        <item name="*com.google.android.apps.walletnfcrel:itemTextAppearanceActive">@*com.google.android.apps.walletnfcrel:style/TextAppearance.GoogleMaterial.Subhead2</item>
        <item name="*com.google.android.apps.walletnfcrel:itemTextAppearanceInactive">@*com.google.android.apps.walletnfcrel:style/TextAppearance.GoogleMaterial.Subhead2</item>
        <item name="*com.google.android.apps.walletnfcrel:itemTextColor">@*com.google.android.apps.walletnfcrel:color/google_bottom_nav_text_color</item>
    </style>	
	<style name="CardViewStyle">
		<item name="android:backgroundTint">@null</item>
        <item name="android:layout_marginLeft">@*com.google.android.apps.walletnfcrel:dimen/card_view_margin</item>
        <item name="android:layout_marginTop">@*com.google.android.apps.walletnfcrel:dimen/very_narrow_spacing</item>
        <item name="android:layout_marginRight">@*com.google.android.apps.walletnfcrel:dimen/card_view_margin</item>
        <item name="android:layout_marginBottom">@*com.google.android.apps.walletnfcrel:dimen/very_narrow_spacing</item>
        <item name="*com.google.android.apps.walletnfcrel:cardCornerRadius">@*com.google.android.apps.walletnfcrel:dimen/card_corner_radius</item>
        <item name="*com.google.android.apps.walletnfcrel:cardElevation">0.0dip</item>
    </style>
	<style name="CardView" parent="@*com.google.android.apps.walletnfcrel:style/Base.CardView" >
		<item name="android:backgroundTint">@*android:color/lighter_gray</item>
    </style>
    <style name="Base.CardView" parent="@android:style/Widget">
        <item name="*com.google.android.apps.walletnfcrel:cardCornerRadius">@*com.google.android.apps.walletnfcrel:dimen/cardview_default_radius</item>
        <item name="*com.google.android.apps.walletnfcrel:cardElevation">0.0dip</item>
        <item name="*com.google.android.apps.walletnfcrel:cardMaxElevation">0.0dip</item>
        <item name="*com.google.android.apps.walletnfcrel:cardPreventCornerOverlap">true</item>
        <item name="*com.google.android.apps.walletnfcrel:cardUseCompatPadding">false</item>
		<item name="android:backgroundTint">@*android:color/lighter_gray</item>
    </style>
    <style name="DividerStyle">
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@*com.google.android.apps.walletnfcrel:dimen/account_menu_divider_height</item>
        <item name="android:contentDescription">@null</item>
    </style>
    <style name="HorizontalDivider">
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
    </style>
    <style name="HorizontalDivider.Black12" parent="@*com.google.android.apps.walletnfcrel:style/HorizontalDivider">
        <item name="android:background">@null</item>
    </style>
    <style name="HorizontalDividerLight">
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
    </style>
</resources>