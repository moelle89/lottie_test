<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat.Large">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@*android:color/accent_material_dark</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat.Small">
        <item name="android:textColor">?android:textColorTertiaryInverse</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Subhead.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Title.Inverse" parent="@*com.samsung.android.messaging:style/Base.TextAppearance.AppCompat.Title">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHint">@*android:color/holo_button_pressed</item>
    </style>
    <style name="CommonDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowMinWidthMajor">@*android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*android:dimen/dialog_min_width_minor</item>
        <item name="android:colorControlActivated">@*com.samsung.android.messaging:color/tw_checkbox_on</item>
        <item name="android:colorPrimary">@*android:color/accent_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/accent_material_dark</item>
    </style>
    <style name="AppBaseTheme" parent="@*android:style/Theme.Material" />
    <style name="ListViewStyle" parent="@*android:style/Widget.DeviceDefault.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="MsgFullDialogStyle" parent="@*android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowBackground">@*com.samsung.android.messaging:drawable/winset_picker_dialog_background</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowSoftInputMode">adjustResize|stateHidden</item>
        <item name="android:colorPrimary">@*com.samsung.android.messaging:color/new_action_bar_colour</item>
        <item name="android:colorPrimaryDark">@*android:color/accent_material_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:windowElevation">0.0dip</item>
        <item name="android:colorEdgeEffect">@*android:color/holo_button_pressed</item>
    </style>
    <style name="MsgFullDialogTabStyle" parent="@*android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowBackground">@*com.samsung.android.messaging:drawable/winset_picker_dialog_background</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowSoftInputMode">adjustResize|stateHidden</item>
        <item name="android:colorPrimary">@*com.samsung.android.messaging:color/new_action_bar_colour</item>
        <item name="android:colorPrimaryDark">@*android:color/accent_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/holo_button_pressed</item>
    </style>
    <style name="ClusterIcon.TextAppearance" parent="@*android:style/TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Bubble.TextAppearance.Dark" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Bubble.TextAppearance.Light" parent="@*com.samsung.android.messaging:style/Bubble.TextAppearance.Dark">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:textColorHighlight">@*android:color/holo_button_pressed</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
    </style>
</resources>
