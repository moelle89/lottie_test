<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.BillingActionBar" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:backgroundDimAmount">@android:color/transparent</item>
    </style>
    <style name="Theme.BillingTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionBarStyle">@*com.samsung.android.scloud:style/Theme.BillingActionBar</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.DashBoardBurstShotDetail" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.SCloudTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionOverflowButtonStyle">@*com.samsung.android.scloud:style/OverFlowButtonStyle</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:colorControlActivated">@*android:color/accent_material_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.SettingPreference" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionBarStyle">@*com.samsung.android.scloud:style/GalleryActionBar</item>
        <item name="android:alertDialogTheme">@*com.samsung.android.scloud:style/CustomAlertDialogTheme</item>
        <item name="android:actionMenuTextColor">@*com.samsung.android.scloud:color/actionbar_back_icon_color</item>
        <item name="android:colorControlActivated">@*com.samsung.android.scloud:color/gallery_color_primary</item>
        <item name="android:colorPrimary">@*com.samsung.android.scloud:color/gallery_color_primary</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="CardView.Dark" parent="@*com.samsung.android.scloud:style/CardView">
        <item name="*com.samsung.android.scloud:cardBackgroundColor">@*android:color/background_dark</item>
    </style>
    <style name="CardView.Light" parent="@*com.samsung.android.scloud:style/CardView">
        <item name="*com.samsung.android.scloud:cardBackgroundColor">@*android:color/background_dark</item>
    </style>
    <style name="CardViewBackgroundTheme">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:foreground">@*com.samsung.android.scloud:drawable/cardview_background</item>
        <item name="android:radius">8.0dip</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
    </style>
    <style name="Theme.Setupwizard" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="UploadSheet" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/NoCloseButton</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="DriveTheme" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/NoCloseButton</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="list_divider">
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0px</item>
        <item name="android:insetLeft">0.0dip</item>
        <item name="android:insetRight">0.0dip</item>
    </style>
    <style name="primary_text">
        <item name="android:textSize">@*com.samsung.android.scloud:dimen/list_primary_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">sec-roboto-light</item>
    </style>
    <style name="secondary_text">
        <item name="android:textSize">@*com.samsung.android.scloud:dimen/list_secondary_text_size</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:fontFamily">sec-roboto-light</item>
    </style>
    <style name="styleSyncSettingsSummary">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
</resources>
