<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:tint="#17B3D1E3" >
            <gradient
                android:startColor="#B1000000"
                android:endColor="#53000000"
                android:angle="90" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:tint="@*android:color/facelock_spotlight_mask">
            <gradient android:startColor="@*android:color/facelock_spotlight_mask" android:endColor="#A4000000" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:tint="@*android:color/accent_material_dark">
            <gradient android:endColor="@android:color/transparent" android:centerColor="#62000000"  android:startColor="@*android:color/facelock_spotlight_mask" android:angle="45.0" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:tint="@*android:color/button_material_dark">
            <gradient android:endColor="@android:color/transparent"  android:startColor="#AB000000" android:angle="45.0" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient android:endColor="@*android:color/background_dark" android:startColor="#0BA6CDE4" android:angle="90.0" />
        </shape>
    </item>
    <item android:gravity="bottom" android:height="1.0dip">
        <shape android:shape="rectangle" android:tint="@*android:color/holo_orange_light">
            <gradient android:endColor="#59000000"  android:startColor="#00000000" android:angle="45.0" />
        </shape>
    </item>
    <item android:gravity="bottom" android:height="1.0dip">
        <shape android:shape="rectangle" android:tint="@*android:color/accessibility_focus_highlight">
            <gradient android:endColor="#00000000"  android:startColor="#77000000" android:angle="45.0" />
        </shape>
    </item>
</layer-list>