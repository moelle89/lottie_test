        <item name="android:actionMenuTextColor">@*com.samsung.android.contacts:color/contact_action_bar_text_color</item>
        <item name="android:colorControlActivated">@*com.samsung.android.contacts:color/contact_color_control_activated</item>
        <item name="android:colorPrimary">@*android:color/accent_material_dark</item>
