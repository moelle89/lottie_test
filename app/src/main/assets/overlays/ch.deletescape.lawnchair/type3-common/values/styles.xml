<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AllAppsNavBarProtection">
        <item name="android:background">@*ch.deletescape.lawnchair:drawable/notification_bg_low_normal</item>
		<item name="android:backgroundTint">@null</item>
    </style>
    <style name="SettingsTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="*ch.deletescape.lawnchair:blurTintColor">@*android:color/legacy_primary</item>
    </style>
    <style name="SettingsTheme.Black" parent="@*ch.deletescape.lawnchair:style/SettingsTheme.Dark">
		<item name="android:windowBackground">@*android:color/holo_primary_dark</item>
		<item name="android:navigationBarColor">@*android:color/holo_primary_dark</item>
        <item name="android:listDivider">@android:color/transparent</item>
		<item name="android:colorPrimary">@*android:color/timepicker_default_background_material</item>
		<item name="android:statusBarColor">@*android:color/timepicker_default_background_material</item>
        <item name="android:colorPrimaryDark">@*android:color/timepicker_default_background_material</item>
    </style>
    <style name="SettingsTheme.Dark" parent="@*android:style/Theme.DeviceDefault">
		<item name="android:windowBackground">@*android:color/user_icon_default_gray</item>
		<item name="android:navigationBarColor">@*android:color/transparent</item>
		<item name="android:colorPrimary">@*android:color/holo_primary_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/holo_primary_dark</item>
		<item name="android:statusBarColor">@*android:color/holo_primary_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
		<item name="android:listDivider">@android:color/transparent</item>
        <item name="*ch.deletescape.lawnchair:blurTintColor">@*android:color/legacy_primary</item>
    </style>

    <style name="BaseLauncherTheme" parent="@*android:style/Theme.DeviceDefault.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="BaseLauncherTheme.Dark" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:backgroundTint">@*android:color/secondary_text_default_material_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="LauncherTheme" parent="@*ch.deletescape.lawnchair:style/BaseLauncherTheme">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#00000000</item>
        <item name="android:navigationBarColor">#00000000</item>
        <item name="*ch.deletescape.lawnchair:allAppsContainerColor">@*android:color/background_dark</item>
        <item name="*ch.deletescape.lawnchair:allAppsContainerColorBlur">#00000000</item>
        <item name="*ch.deletescape.lawnchair:allAppsEmptySearchTextColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair:appPopupBgColor">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair:appPopupHeaderBgColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:appPopupTextColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair:blurTintColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:deepShortcutsHandleColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair:folderBgColorBlur">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:folderBgIntensity">75</item>
        <item name="*ch.deletescape.lawnchair:folderIconTextColor">@*android:color/lighter_gray</item>
        <item name="*ch.deletescape.lawnchair:folderNameTextColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="*ch.deletescape.lawnchair:materialPanelBgColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:notificationBeneathColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:qsbBgColor">@*android:color/dim_foreground_light</item><!--round search widget -->
        <item name="*ch.deletescape.lawnchair:roundSearchBarBgColor">@*android:color/highlighted_text_dark</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarDividerColor">#03eeeeee</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarHintColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarTextColor">@android:color/white</item>
    </style>
    <style name="LauncherTheme.Black" parent="@*ch.deletescape.lawnchair:style/LauncherTheme.Dark">
        <item name="*ch.deletescape.lawnchair:allAppsContainerColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:allAppsContainerColorBlur">#00000000</item>
        <item name="*ch.deletescape.lawnchair:appPopupBgColor">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair:appPopupHeaderBgColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:blurTintColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:folderBgColorBlur">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:folderBgIntensity">0</item>
        <item name="*ch.deletescape.lawnchair:materialPanelBgColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:notificationBeneathColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:qsbBgColor">@*android:color/dim_foreground_light</item>
    </style>
    <style name="LauncherTheme.Dark" parent="@*ch.deletescape.lawnchair:style/BaseLauncherTheme.Dark">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#00000000</item>
        <item name="android:navigationBarColor">#00000000</item>
        <item name="*ch.deletescape.lawnchair:allAppsContainerColor">@*android:color/background_dark</item>
        <item name="*ch.deletescape.lawnchair:allAppsContainerColorBlur">#00000000</item>
        <item name="*ch.deletescape.lawnchair:allAppsEmptySearchTextColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair:appPopupBgColor">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair:appPopupHeaderBgColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:appPopupTextColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair:blurTintColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:deepShortcutsHandleColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair:folderBgColorBlur">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:folderBgIntensity">75</item>
        <item name="*ch.deletescape.lawnchair:folderIconTextColor">@*android:color/lighter_gray</item>
        <item name="*ch.deletescape.lawnchair:folderNameTextColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="*ch.deletescape.lawnchair:materialPanelBgColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair:notificationBeneathColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair:qsbBgColor">@*android:color/dim_foreground_light</item><!--round search widget -->
        <item name="*ch.deletescape.lawnchair:roundSearchBarBgColor">@*android:color/highlighted_text_dark</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarDividerColor">#03eeeeee</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarHintColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair:roundSearchBarTextColor">@android:color/white</item>
    </style>
</resources>