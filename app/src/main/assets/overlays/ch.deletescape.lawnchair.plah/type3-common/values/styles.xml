<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AllAppsNavBarProtection">
        <item name="android:background">@*ch.deletescape.lawnchair.plah:drawable/notification_bg_low_normal</item>
		<item name="android:backgroundTint">@null</item>
    </style>
    <style name="SettingsTheme.Black" parent="@*ch.deletescape.lawnchair.plah:style/SettingsTheme.Dark">
        <item name="android:windowBackground">@*android:color/background_dark</item>        
        <item name="android:listDivider">@android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:colorPrimary">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair.plah:colorPrimaryDark">@*android:color/timepicker_default_background_material</item>
    </style>
    <style name="SettingsTheme.Dark" parent="@*ch.deletescape.lawnchair.plah:style/Theme.AppCompat">
        <item name="android:navigationBarColor">@*android:color/transparent</item>
        <item name="*ch.deletescape.lawnchair.plah:blurTintColor">@*android:color/facelock_spotlight_mask</item>
        <item name="*ch.deletescape.lawnchair.plah:colorAccent">@*android:color/accent_material_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:colorPrimary">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair.plah:colorPrimaryDark">@*android:color/timepicker_default_background_material</item>
    </style>

    <style name="BaseLauncherTheme.Dark" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="LauncherTheme.Black" parent="@*ch.deletescape.lawnchair.plah:style/LauncherTheme.Dark">
        <item name="*ch.deletescape.lawnchair.plah:allAppsContainerColor">@*android:color/black</item>
        <item name="*ch.deletescape.lawnchair.plah:allAppsContainerColorBlur">#00000000</item>
        <item name="*ch.deletescape.lawnchair.plah:appPopupHeaderBgColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair.plah:blurTintColor">@*android:color/facelock_spotlight_mask</item>
        <item name="*ch.deletescape.lawnchair.plah:folderBgColorBlur">@*android:color/facelock_spotlight_mask</item>
        <item name="*ch.deletescape.lawnchair.plah:folderBgIntensity">0</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorPrimary">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorSecondary">@*android:color/button_material_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorTertiary">#ffffffff</item>
        <item name="*ch.deletescape.lawnchair.plah:materialPanelBgColor">@*android:color/facelock_spotlight_mask</item>
        <item name="*ch.deletescape.lawnchair.plah:qsbBgColor">@*android:color/dim_foreground_light</item>
    </style>
    <style name="LauncherTheme.Dark" parent="@*ch.deletescape.lawnchair.plah:style/BaseLauncherTheme.Dark">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#00000000</item>
        <item name="android:navigationBarColor">#00000000</item>
        <item name="*ch.deletescape.lawnchair.plah:allAppsContainerColor">@*android:color/background_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:allAppsContainerColorBlur">#00000000</item>
        <item name="*ch.deletescape.lawnchair.plah:allAppsEmptySearchTextColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair.plah:appPopupHeaderBgColor">@*android:color/timepicker_default_background_material</item>
        <item name="*ch.deletescape.lawnchair.plah:appPopupTextColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair.plah:blurTintColor">@*android:color/facelock_spotlight_mask</item>
        <item name="*ch.deletescape.lawnchair.plah:deepShortcutsHandleColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair.plah:folderBgColorBlur">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair.plah:folderBgIntensity">75</item>
        <item name="*ch.deletescape.lawnchair.plah:folderIconTextColor">@*android:color/lighter_gray</item>
        <item name="*ch.deletescape.lawnchair.plah:folderNameTextColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:materialPanelBgColor">@*android:color/legacy_primary</item>
        <item name="*ch.deletescape.lawnchair.plah:qsbBgColor">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair.plah:roundSearchBarBgColor">@*android:color/highlighted_text_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:roundSearchBarDividerColor">#03eeeeee</item>
        <item name="*ch.deletescape.lawnchair.plah:roundSearchBarHintColor">@android:color/white</item>
        <item name="*ch.deletescape.lawnchair.plah:roundSearchBarTextColor">@android:color/white</item>
		<item name="*ch.deletescape.lawnchair.plah:searchBarTextColor">?android:textColorSecondary</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorPrimary">@*android:color/dim_foreground_light</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorSecondary">@*android:color/button_material_dark</item>
        <item name="*ch.deletescape.lawnchair.plah:popupColorTertiary">#ffffffff</item>
    </style>
</resources>