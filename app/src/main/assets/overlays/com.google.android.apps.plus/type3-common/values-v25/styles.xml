<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextStyle.ActivityLog.Timestamp" parent="@*com.google.android.apps.plus:style/TextStyle.Regular">
        <item name="android:textSize">@*com.google.android.apps.plus:dimen/textsize_12</item>
        <item name="android:textColor">@*android:color/lighter_gray</item>
		<item name="android:textStyle">bold</item>
    </style>
    <style name="TextStyle.SocialPost.Timestamp" parent="@*com.google.android.apps.plus:style/TextStyle.Regular">
        <item name="android:textSize">@*com.google.android.apps.plus:dimen/textsize_12</item>
        <item name="android:textColor">@*android:color/lighter_gray</item>
    </style>
    <style name="TextStyle.CommentCard.Timestamp" parent="@*com.google.android.apps.plus:style/TextStyle.Regular">
        <item name="android:textSize">@*com.google.android.apps.plus:dimen/textsize_12</item>
        <item name="android:textColor">@*android:color/lighter_gray</item>
    </style>
	<style name="Theme.SharekitReshare.BottomSheet" parent="@android:style/Theme.Material.NoActionBar">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowActionBar">false</item>
		<item name="android:statusBarColor">@*android:color/transparent</item>
		<item name="android:backgroundTint">@*android:color/timepicker_default_background_material</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:backgroundDimEnabled">true</item>
		<item name="android:navigationBarColor">@*android:color/timepicker_default_background_material</item>
	</style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@android:style/Widget">
        <item name="android:background">@*android:color/timepicker_default_background_material</item>
        <item name="android:elevation">@*com.google.android.apps.plus:dimen/design_bottom_sheet_modal_elevation</item>
		<item name="android:navigationBarColor">@*android:color/timepicker_default_background_material</item>
        <item name="*com.google.android.apps.plus:behavior_peekHeight">auto</item>
        <item name="*com.google.android.apps.plus:behavior_hideable">true</item>
        <item name="*com.google.android.apps.plus:behavior_skipCollapsed">false</item>
    </style>
    <style name="Platform.V25.AppCompat.Light" parent="@android:style/Theme.Material.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:backgroundTint">@*android:color/background_material_dark</item>
		<item name="android:colorAccent">@*android:color/Red_700</item>
		<item name="android:colorControlHighlight">@*android:color/Red_700</item>
        <item name="android:colorControlActivated">@*android:color/Red_700</item>
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_material_dark</item>
    </style>
    <style name="Platform.V25.AppCompat" parent="@android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:backgroundTint">@null</item>
		<item name="android:navigationBarColor">@*android:color/background_material_dark</item>
		<item name="android:colorAccent">@*android:color/Red_700</item>
		<item name="android:colorControlHighlight">@*android:color/Red_700</item>
        <item name="android:colorControlActivated">@*android:color/Red_700</item>
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="Theme.Plus.Launcher" parent="@*android:style/Theme.Material.DialogWhenLarge">
        <item name="android:windowBackground">@*com.google.android.apps.plus:drawable/quantum_launchscreen_google_plus</item>
        <item name="android:backgroundTint">@null</item>
		<item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
		<item name="android:colorAccent">@*android:color/Red_700</item>
		<item name="android:colorControlHighlight">@*android:color/Red_700</item>
        <item name="android:colorControlActivated">@*android:color/Red_700</item>
		<item name="android:statusBarColor">@*android:color/background_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_material_dark</item>
    </style>
    <style name="Theme.Plus.Launcher.NoActionBar" parent="@*com.google.android.apps.plus:style/Theme.Arkham.NoActionBar">
        <item name="android:windowBackground">@*com.google.android.apps.plus:drawable/quantum_launchscreen_google_plus</item>
        <item name="android:backgroundTint">@null</item>
		<item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
		<item name="android:colorAccent">@*android:color/Red_700</item>
		<item name="android:colorControlHighlight">@*android:color/Red_700</item>
        <item name="android:colorControlActivated">@*android:color/Red_700</item>
		<item name="android:statusBarColor">@*android:color/background_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_material_dark</item>
    </style>
	<style name="Theme.Plus.SpinnerBlack" parent="@*com.google.android.apps.plus:style/Theme.Arkham">
        <item name="*com.google.android.apps.plus:colorControlNormal">?android:colorControlNormal</item>
        <item name="*com.google.android.apps.plus:colorControlActivated">?android:colorControlActivated</item>
        <item name="*com.google.android.apps.plus:colorControlHighlight">?android:colorControlHighlight</item>
		<item name="android:backgroundTint">@*android:color/white</item>
    </style>
</resources>
