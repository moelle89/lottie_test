<?xml version="1.0" encoding="utf-8"?>
<com.google.android.libraries.social.spaces.SpaceListItemView
    android:focusable="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minWidth="@*com.google.android.apps.plus:dimen/space_list_item_view_min_width"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.google.android.libraries.social.media.ui.MediaView
        android:id="@*com.google.android.apps.plus:id/photo"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" size="custom" scale="zoom" />
    <TextView
        android:textColor="#000000"
        android:gravity="center"
        android:id="@*com.google.android.apps.plus:id/unread_count"
        android:background="@*com.google.android.apps.plus:drawable/solid_circle"
        android:visibility="gone"
        android:layout_width="28.0dip"
        android:layout_height="28.0dip"
        style="@*com.google.android.apps.plus:style/TextStyle.PlusOne.SecondaryText.Medium" />
    <com.google.android.libraries.social.ui.views.linearavatarpileview.LinearAvatarPileView
        android:id="@*com.google.android.apps.plus:id/avatar_pile"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="@*com.google.android.apps.plus:dimen/space_list_item_avatar_with_border_height" />
    <TextView
        android:ellipsize="end"
        android:id="@*com.google.android.apps.plus:id/title"
        android:paddingLeft="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:paddingRight="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="2"
        android:lineSpacingExtra="@*com.google.android.apps.plus:dimen/space_list_item_view_title_line_spacing_extra"
        android:lineSpacingMultiplier="@*com.google.android.apps.plus:dimen/space_list_item_view_title_line_spacing_multiplier"
        style="@*com.google.android.apps.plus:style/quantum_text_subhead_white" />
    <TextView
        android:ellipsize="end"
        android:id="@*com.google.android.apps.plus:id/owner_or_visibility"
        android:paddingLeft="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:paddingRight="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:drawablePadding="4.0dip"
        android:lineSpacingExtra="@*com.google.android.apps.plus:dimen/space_list_item_view_title_line_spacing"
        android:lineSpacingMultiplier="@*com.google.android.apps.plus:dimen/space_list_item_view_owner_or_visibility_line_spacing_multiplier"
        style="@*com.google.android.apps.plus:style/quantum_text_caption_white" />
    <Button
        android:textSize="@*com.google.android.apps.plus:dimen/space_header_secondary_text_size"
        android:textColor="@*com.google.android.apps.plus:color/quantum_black_text"
        android:gravity="start|bottom|center"
        android:id="@*com.google.android.apps.plus:id/primary_button"
        android:background="@*com.google.android.apps.plus:drawable/transparent_follow_button"
        android:padding="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="0.0dip"
        android:maxLines="1"
        android:textAllCaps="true" />
    <LinearLayout
        android:layout_gravity="start|bottom|center"
        android:orientation="horizontal"
        android:id="@*com.google.android.apps.plus:id/checkbox_container"
        android:padding="10.0dip"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <CheckBox
            android:id="@*com.google.android.apps.plus:id/checkbox_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:textSize="@*com.google.android.apps.plus:dimen/space_header_secondary_text_size"
            android:textColor="@*com.google.android.apps.plus:color/quantum_black_text"
            android:id="@*com.google.android.apps.plus:id/checkbox_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true" />
    </LinearLayout>
    <ImageButton
        android:id="@*com.google.android.apps.plus:id/dismiss_button"
        android:background="@null"
        android:padding="@*com.google.android.apps.plus:dimen/space_list_item_view_padding"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@*com.google.android.apps.plus:drawable/quantum_ic_close_white_18"
        android:contentDescription="@*com.google.android.apps.plus:string/spaces_list_item_dismiss_content_description"
        android:alpha="0.54" />
</com.google.android.libraries.social.spaces.SpaceListItemView>