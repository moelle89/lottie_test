<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CardView.Dark" parent="@*com.apkupdater:style/CardView">
        <item name="*com.apkupdater:cardBackgroundColor">@*android:color/transparent</item>        
		<item name="android:backgroundTint">@*android:color/dim_foreground_light</item>
    </style>
    <style name="Widget.Design.FloatingActionButton" parent="@android:style/Widget">
        <item name="android:background">@*com.apkupdater:drawable/design_fab_background</item>
        <item name="*com.apkupdater:elevation">@*com.apkupdater:dimen/design_fab_elevation</item>
        <item name="*com.apkupdater:rippleColor">?android:colorControlHighlight</item>
        <item name="*com.apkupdater:fabSize">auto</item>
        <item name="*com.apkupdater:pressedTranslationZ">@*com.apkupdater:dimen/design_fab_translation_z_pressed</item>
        <item name="*com.apkupdater:borderWidth">@*com.apkupdater:dimen/design_fab_border_width</item>
        <item name="*com.apkupdater:backgroundTint">@null</item>
    </style>
</resources>
