<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.AppCompat.Notification" parent="@android:style/TextAppearance.Material.Notification">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffa5bfcc</item>
    </style>
    <style name="TextAppearance.AppCompat.Notification.Title" parent="@android:style/TextAppearance.Material.Notification.Title">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="SnapchatTheme" parent="@*android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:windowBackground">@*com.snapchat.android:drawable/splash_background</item>
        <item name="android:dropDownSpinnerStyle">@*com.snapchat.android:style/SpinnerLightCustom</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="*com.snapchat.android:snapchatTimePickerStyle">@*com.snapchat.android:style/STPWidget.Holo.SnapchatTimePicker</item>
		<item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>
</resources>