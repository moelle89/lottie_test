<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false"
            android:visible="true">
			<solid android:color="#00008ec2" />
            <corners android:radius="25.0dip" />
            <size android:width="21.0dip" android:height="21.0dip" />
            <stroke android:width="1.2dp" android:color="@*android:color/material_blue_grey_700" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false"
            android:visible="true">
			<solid android:color="#00151515" />
            <corners android:radius="25.0dip" />
            <size android:width="21.0dip" android:height="21.0dip" />
            <stroke android:width="1.0dp"
                android:color="@*android:color/material_blue_grey_700" />
        </shape>
    </item>
</selector>