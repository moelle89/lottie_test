    <style name="InCallDialpad" parent="@*com.google.clone.dialer:style/Dialpad.Light">
		<item name="android:backgroundTint">@*android:color/facelock_spotlight_mask</item>
        <item name="*com.google.clone.dialer:dialpad_end_key_spacing">100.0dip</item>
        <item name="*com.google.clone.dialer:dialpad_digits_adjustable_text_size">20.0sp</item>
        <item name="*com.google.clone.dialer:dialpad_digits_adjustable_height">50.0dip</item>
        <item name="*com.google.clone.dialer:dialpad_key_numbers_size">32.0sp</item>
        <item name="*com.google.clone.dialer:dialpad_key_number_margin_bottom">1.0dip</item>
        <item name="*com.google.clone.dialer:dialpad_key_letters_size_for_dual_alphabets">@*com.google.clone.dialer:dimen/incall_dialpad_key_letters_size_for_dual_alphabets</item>
