		<item name="android:windowShowWallpaper">false</item>
		<item name="android:colorBackground">@*android:color/dim_foreground_light</item>
    </style>
    <style name="FullWidthDivider">
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
    </style>
	
    <style name="Widget.Design.FloatingActionButton" parent="@android:style/Widget">
        <item name="android:background">@*com.google.clone.dialer:drawable/design_fab_background</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="*com.google.clone.dialer:elevation">@*com.google.clone.dialer:dimen/design_fab_elevation</item>
        <item name="*com.google.clone.dialer:rippleColor">@*android:color/holo_button_pressed</item>
        <item name="*com.google.clone.dialer:fabSize">auto</item>
        <item name="*com.google.clone.dialer:hoveredFocusedTranslationZ">@*com.google.clone.dialer:dimen/design_fab_translation_z_hovered_focused</item>
        <item name="*com.google.clone.dialer:pressedTranslationZ">@*com.google.clone.dialer:dimen/design_fab_translation_z_pressed</item>
        <item name="*com.google.clone.dialer:borderWidth">@*com.google.clone.dialer:dimen/design_fab_border_width</item>
        <item name="*com.google.clone.dialer:maxImageSize">@*com.google.clone.dialer:dimen/design_fab_image_size</item>
        <item name="*com.google.clone.dialer:showMotionSpec">@*com.google.clone.dialer:animator/design_fab_show_motion_spec</item>
        <item name="*com.google.clone.dialer:hideMotionSpec">@*com.google.clone.dialer:animator/design_fab_hide_motion_spec</item>
        <item name="*com.google.clone.dialer:backgroundTint">@*android:color/accent_material_dark</item>
