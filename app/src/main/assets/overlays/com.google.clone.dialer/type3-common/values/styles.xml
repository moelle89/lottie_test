    <style name="Dialpad">
        <item name="*com.google.clone.dialer:dialpad_end_key_spacing">@*com.google.clone.dialer:dimen/dialpad_bottom_space_height</item>
        <item name="*com.google.clone.dialer:dialpad_digits_adjustable_text_size">@*com.google.clone.dialer:dimen/dialpad_digits_text_size</item>
        <item name="*com.google.clone.dialer:dialpad_digits_adjustable_height">@*com.google.clone.dialer:dimen/dialpad_digits_height</item>
        <item name="*com.google.clone.dialer:dialpad_key_numbers_size">@*com.google.clone.dialer:dimen/dialpad_key_numbers_default_size</item>
        <item name="*com.google.clone.dialer:dialpad_key_number_margin_bottom">@*com.google.clone.dialer:dimen/dialpad_key_number_default_margin_bottom</item>
        <item name="*com.google.clone.dialer:dialpad_key_letters_size_for_dual_alphabets">@*com.google.clone.dialer:dimen/dialpad_key_letters_default_size_for_dual_alphabets</item>
