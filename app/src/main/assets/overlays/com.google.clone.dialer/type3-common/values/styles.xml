    <style name="Dialpad.Light" parent="@*com.google.clone.dialer:style/Dialpad">
		<item name="android:backgroundTint">@null</item>
        <item name="*com.google.clone.dialer:dialpad_text_color">#ffffffff</item>
        <item name="*com.google.clone.dialer:dialpad_text_color_primary">#ffffffff</item>
        <item name="*com.google.clone.dialer:dialpad_text_color_secondary">@*android:color/accent_material_dark</item>
        <item name="*com.google.clone.dialer:dialpad_icon_tint">@null</item>
        <item name="*com.google.clone.dialer:dialpad_voicemail_tint">@*android:color/accent_material_dark</item>
        <item name="*com.google.clone.dialer:dialpad_background">@*com.google.clone.dialer:drawable/search_shadow</item>
    </style>
	
    <style name="Theme.InCallScreen" parent="@*com.google.clone.dialer:style/Theme.AppCompat.NoActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffaac0ca</item>
        <item name="android:windowBackground">@*com.google.clone.dialer:drawable/incall_background_gradient</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:alertDialogTheme">@*com.google.clone.dialer:style/AlertDialogTheme</item>
        <item name="android:colorPrimary">@*android:color/holo_primary_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/holo_primary_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="*com.google.clone.dialer:dialpad_style">@*com.google.clone.dialer:style/InCallDialpad</item>
        <item name="*com.google.clone.dialer:dialpad_key_button_touch_tint">@*android:color/holo_button_pressed</item>
    </style>
	
	<style name="DialpadKeyLettersStyle">
