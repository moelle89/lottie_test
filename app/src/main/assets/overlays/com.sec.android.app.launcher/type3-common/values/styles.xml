<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AboutPageNoDividerTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.sec.android.app.launcher:style/NoDividerActionBar</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="SettingsTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.sec.android.app.launcher:style/SettingsActionBar</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="HomeAppSettingsTheme" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@*com.sec.android.app.launcher:style/LauncherActionBar</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="AddItemDialog" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Theme" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
    <style name="LauncherActionBar" parent="@*android:style/Widget.Material.ActionBar">
        <item name="android:displayOptions">showTitle</item>
        <item name="android:contentInsetStart">0.0dip</item>
    </style>
    <style name="LoadingTheme" parent="@*android:style/Theme.Material.Panel">
        <item name="android:windowBackground">@*android:color/transparent</item>
    </style>
</resources>