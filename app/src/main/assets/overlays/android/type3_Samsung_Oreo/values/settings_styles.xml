<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Theme.DeviceDefault.QuickSettings" parent="@android:style/Theme.DeviceDefault" >
        <item name="android:colorControlNormal">#ffffffff</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorSecondary">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="Theme.Material.Settings" parent="@android:style/Theme.Material">
        <item name="android:textColorPrimary">@*android:color/primary_text_material_dark</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_material_dark</item>
        <item name="android:colorForeground">@*android:color/white</item>
    	<item name="android:windowBackground">@*android:color/background_dark</item>
		<item name="android:windowShowWallpaper">false</item>
	    <item name="android:listDivider">@*android:color/transparent</item>
	    <item name="android:colorSecondary">#0cb3d1e3</item>
		<item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorControlActivated">@*android:color/accent_material_dark</item>
		<item name="android:colorEdgeEffect">@*android:color/holo_button_pressed</item>
		<item name="android:colorControlHighlight">@*android:color/holo_button_pressed</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
		<item name="android:colorControlNormal">@*android:color/holo_orange_light</item>
		<item name="android:presentationTheme">@*android:style/Theme.Material.Settings.Dialog.Presentation</item>
        <item name="*android:panelMenuListTheme">@*android:style/Theme.Material.Settings.CompactMenu</item>
        <item name="*android:searchDialogTheme">@*android:style/Theme.Material.Settings.SearchBar</item>
    </style>
    <style name="Theme.Material.Settings.NoActionBar" parent="@*android:style/Theme.Material.NoActionBar">
		<item name="android:colorControlActivated">@*android:color/accent_material_dark</item>
		<item name="android:colorControlNormal">@*android:color/holo_orange_light</item>
		<item name="android:colorBackground">@*android:color/background_dark</item>
		<item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
		<item name="android:colorControlHighlight">@*android:color/holo_button_pressed</item>
	    <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
    <style name="Theme.Material.Settings.BaseDialog" parent="@*android:style/Theme.Material.BaseDialog"/>
    <style name="Theme.Material.Settings.Dialog.BaseAlert" parent="@*android:style/Theme.Material.Dialog.BaseAlert"/>
    <style name="Theme.Material.Settings.DialogWhenLarge.NoActionBar" parent="@android:style/Theme.Material.DialogWhenLarge.NoActionBar"/>
    <style name="Theme.Material.Settings.Dialog.Presentation" parent="@android:style/Theme.Material.Dialog.Presentation"/>
	
    <style name="Theme.DeviceDefault.Settings" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:textColorPrimary">@*android:color/primary_text_material_dark</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_material_dark</item>
        <item name="android:colorForeground">@*android:color/white</item>
		<item name="android:colorControlNormal">@*android:color/holo_orange_light</item>
        <item name="android:actionBarTheme">@*android:style/ThemeOverlay.DeviceDefault.ActionBar.Accent</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:popupTheme">@*android:style/ThemeOverlay.Material.Dark</item>
        <item name="android:colorEdgeEffect">@*android:color/holo_button_pressed</item>
        <item name="android:colorSecondary">@*android:color/primary_dark_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
	<style name="Theme.DeviceDefault.Settings.DialogWhenLarge" parent="@*android:style/Theme.Material.DialogWhenLarge">
		<item name="android:windowShowWallpaper">false</item>
		<item name="android:colorBackground">@*android:color/background_dark</item>
	    <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_dark</item>
	</style>
    <style name="Theme.DeviceDefault.Settings.Dark" parent="@android:style/Theme.DeviceDefault">
    	<item name="android:windowBackground">@*android:color/legacy_primary</item>
		<item name="android:windowShowWallpaper">false</item>
		<item name="android:colorControlNormal">@*android:color/holo_orange_light</item>
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/transparent</item>
    </style>
    <style name="Theme.DeviceDefault.NoActionBar" parent="@*android:style/Theme.Material.NoActionBar" >
		<item name="android:alertDialogStyle">@*android:style/AlertDialog.DeviceDefault.Light</item>
        <item name="android:dialogTheme">@*android:style/Theme.DeviceDefault.Light.Dialog</item>
        <item name="android:alertDialogTheme">@*android:style/Theme.DeviceDefault.Light.Dialog.Alert</item>
		<item name="android:windowShowWallpaper">false</item>
		<item name="android:windowBackground">@*android:color/legacy_primary</item>
		<item name="android:colorControlNormal">@*android:color/holo_orange_light</item>
		<item name="android:colorControlActivated">@*android:color/accent_material_dark</item>
		<item name="android:colorEdgeEffect">@*android:color/accent_material_dark</item>
		<item name="android:colorControlHighlight">@*android:color/holo_button_pressed</item>
		<item name="android:colorBackground">@*android:color/transparent</item>
    </style>
