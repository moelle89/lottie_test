		<item name="android:windowIsTranslucent">false</item>
		<item name="android:backgroundTint">@*android:color/background_device_default_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_device_default_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
		<item name="android:colorBackground">@*android:color/background_dark</item>
		<item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>        
		<item name="*com.google.android.dialer:colorPrimaryDark">@*android:color/background_dark</item>
		<item name="android:statusBarColor">@*android:color/background_dark</item>
    </style>
    <style name="Platform.V25.AppCompat" parent="@*android:style/Theme.Material.NoActionBar">
		<item name="android:windowIsTranslucent">false</item>
		<item name="android:windowShowWallpaper">@*android:bool/target_honeycomb_needs_options_menu</item>
		<item name="android:backgroundTint">@*android:color/background_device_default_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_device_default_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
		<item name="android:colorBackground">@*android:color/background_dark</item>
		<item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>        
		<item name="*com.google.android.dialer:colorPrimaryDark">@*android:color/background_dark</item>
		<item name="android:statusBarColor">@*android:color/background_dark</item>
    </style>
    <style name="Widget.GoogleMaterial.CardView" parent="@*com.google.android.dialer:style/CardView">
        <item name="*com.google.android.dialer:cardBackgroundColor">@android:color/transparent</item>
		<item name="android:backgroundTint">@android:color/transparent</item>
        <item name="*com.google.android.dialer:cardCornerRadius">8.0dip</item>
        <item name="*com.google.android.dialer:cardElevation">0.0dip</item>
        <item name="*com.google.android.dialer:shapeAppearance">@null</item>
        <item name="*com.google.android.dialer:strokeColor">@android:color/transparent</item>
        <item name="*com.google.android.dialer:strokeWidth">0.0dip</item>
        <item name="*com.google.android.dialer:enforceMaterialTheme">true</item>
    </style>
</resources>