        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.tasks:style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
