<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.OneGoogle.GoogleMaterial.NavigationView" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.NavigationView">
        <item name="*com.google.android.contacts:colorAccent">@*android:color/accent_material_dark</item>
    </style>
    <style name="ContactsContextPopupMenuStyle" parent="@*com.google.android.contacts:style/Widget.AppCompat.PopupMenu.Overflow">
        <item name="android:popupBackground">@*android:color/dim_foreground_light</item>
    </style>
	<style name="BaseAppTheme" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Dark">
	    <item name="*com.google.android.contacts:fab_icon_tint">@*android:color/holo_orange_light</item>
        <item name="*com.google.android.contacts:fab_ripple_color">@*android:color/user_icon_default_white</item>
        <item name="*com.google.android.contacts:button_bg_disabled">@*android:color/button_material_dark</item>
        <item name="*com.google.android.contacts:button_text_disabled">@*android:color/holo_orange_light</item>
        <item name="*com.google.android.contacts:chip_stroke_color">@*android:color/holo_orange_light</item>
        <item name="*com.google.android.contacts:colorAccent">@*android:color/accent_material_dark</item>
        <item name="*com.google.android.contacts:colorControlNormal">@android:color/white</item>
        <item name="*com.google.android.contacts:fab_background_tint">@*android:color/accent_material_dark</item>
    </style>
	<style name="Widget.GoogleMaterial.ProductLockupView.Dark" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.ProductLockupView">
        <item name="*com.google.android.contacts:logoColor">light</item>
        <item name="*com.google.android.contacts:productNameTextColor">@android:color/white</item>
    </style>
    <style name="ContactsPopupTheme" parent="@*com.google.android.contacts:style/Widget.AppCompat.PopupMenu.Overflow">
        <item name="android:colorBackground">@*android:color/dim_foreground_light</item>
    </style>
    <style name="ManageAccountChip" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.Chip.Assistive">
        <item name="android:textColor">@android:color/white</item>
        <item name="*com.google.android.contacts:chipBackgroundColor">@*android:color/user_icon_default_white</item>
        <item name="*com.google.android.contacts:chipStrokeColor">@*android:color/holo_orange_light</item>
        <item name="*com.google.android.contacts:rippleColor">@*android:color/highlighted_text_dark</item>
    </style>
</resources>
