<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="DrawerItemTextActiveStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="DrawerItemTextInactiveStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@*android:color/holo_orange_light</item>
    </style>
    <style name="SingleLineTextStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="ContactEditorAccountsChangedActivityTheme" parent="@android:style/Theme.Material.Dialog.NoActionBar.MinWidth">
		<item name="android:windowCloseOnTouchOutside">true</item>
		<item name="android:colorAccent">@*android:color/accent_material_dark</item>
		<item name="android:colorBackground">@*android:color/user_icon_default_gray</item>
	    <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_default_material_dark</item>
	</style>
    <style name="TextAppearanceMedium" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearanceSmall" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@*android:style/Theme.Material" >
	        <item name="android:navigationBarColor">@*android:color/user_icon_default_gray</item>
    </style>
    <style name="PeopleTheme" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/holo_primary_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/holo_primary_dark</item>
		<item name="android:colorBackgroundFloating">@*android:color/user_icon_default_gray</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:navigationBarColor">@*android:color/user_icon_default_gray</item>
        <item name="android:colorBackground">@*android:color/user_icon_default_gray</item>
    </style>
    <style name="Theme.CallSubjectDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:windowElevation">0.0dip</item>
    </style>

	<style name="Widget.GoogleMaterial.FloatingActionButton" parent="@*com.google.android.contacts:style/Widget.MaterialComponents.FloatingActionButton">
        <item name="*com.google.android.contacts:backgroundTint">@*android:color/highlighted_text_holo_dark</item>
        <item name="*com.google.android.contacts:elevation">0.0dip</item>
        <item name="*com.google.android.contacts:enforceMaterialTheme">false</item>
        <item name="*com.google.android.contacts:hoveredFocusedTranslationZ">@*com.google.android.contacts:dimen/google_fab_translation_z_hovered_focused</item>
        <item name="*com.google.android.contacts:pressedTranslationZ">@*com.google.android.contacts:dimen/google_fab_translation_z_pressed</item>
        <item name="*com.google.android.contacts:rippleColor">@*android:color/user_icon_default_white</item>
        <item name="*com.google.android.contacts:tint">@*android:color/facelock_spotlight_mask</item>
    </style>
    <style name="BaseAppTheme" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Light">
        <item name="*com.google.android.contacts:button_bg_disabled">@*android:color/transparent</item>
        <item name="*com.google.android.contacts:button_text_disabled">@*android:color/holo_orange_light</item>
        <item name="*com.google.android.contacts:chip_stroke_color">@*android:color/transparent</item>
        <item name="*com.google.android.contacts:fab_background_tint">?android:colorAccent</item>
    </style>
    <style name="ContactsTheme.NoActionBar.LaunchScreen" parent="@*android:style/Theme.Material.DialogWhenLarge">
        <item name="android:windowBackground">@*com.google.android.contacts:drawable/quantum_launchscreen_contacts</item>
        <item name="android:windowShowWallpaper">false</item>
		<item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
		<item name="android:statusBarColor">@*android:color/user_icon_default_gray</item>
		<item name="android:colorPrimaryDark">@*android:color/user_icon_default_gray</item>
		<item name="android:navigationBarColor">@*android:color/user_icon_default_gray</item>
	</style>
    <style name="ContactsActionBarSubTitleText" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Subtitle">
        <item name="android:textColor">#ffffffff</item>
	    <item name="android:alpha">1.0</item>
    </style>
    <style name="BaseTextStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
	<style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
		<item name="android:windowActionBar">false</item>
        <item name="android:colorBackgroundFloating">@*android:color/dim_foreground_light</item>
		<item name="android:navigationBarColor">@*android:color/user_icon_default_gray</item>
		<item name="android:colorBackground">@*android:color/user_icon_default_gray</item>
    </style>
    <style name="NonPhoneDialogTheme" parent="@*android:style/Theme.Material.Dialog" />
	<style name="ConfirmAddDetailDialogTheme" parent="@android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:colorBackground">@*android:color/user_icon_default_gray</item>
        <item name="android:windowBackground">@*android:color/user_icon_default_gray</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="AccountTypeNameStyle">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="ContactsActionBarTitleText" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ContactsAlertDialogTheme" parent="@android:style/Theme.Material.Dialog">
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColorSecondary">@*android:color/secondary_text_default_material_dark</item>
		<item name="android:textColor">#ffffff</item>
    </style>
    <!-- Dialog -->
    <style name="Base.Theme.GoogleMaterial.Light.Dialog" parent="@*com.google.android.contacts:style/Base.V14.Theme.GoogleMaterial.Light.Dialog">
        <item name="android:colorBackground">@*android:color/dim_foreground_light</item>
        <item name="*com.google.android.contacts:textColorAlertDialogListItem">#FFF</item>
    </style>

    <!-- Cardview (Stroke Color) -->
    <style name="Widget.GoogleMaterial.CardView.Light" parent="@*com.google.android.contacts:style/Widget.GoogleMaterial.CardView">
        <item name="*com.google.android.contacts:strokeColor">@*android:color/user_icon_default_white</item>
    </style>
</resources>
