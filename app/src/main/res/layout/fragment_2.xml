                    android:id="@+id/accent6"
                    android:layout_width="0dp"
                    android:layout_height="83dip"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/accent6"
                    app:cardCornerRadius="@dimen/cardCornerRadius_small"
                    app:cardElevation="?cardelevation_tiny"
                    app:cardUseCompatPadding="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/accent_name_bg"
                        android:layout_gravity="bottom|start"
                        android:background="@drawable/gradient_card"
                        android:padding="@dimen/spacing_small">

                        <TextView
                            android:layout_width="wrap_content"
