                            android:text="@string/accent17"
                            android:textAlignment="textStart"
                            android:textColor="?textcolor"
                            android:textSize="@dimen/accent_name" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:orientation="horizontal"
                tools:visibility="invisible">

                <androidx.cardview.widget.CardView
                    android:id="@+id/accent19"
                    android:layout_width="0dp"
                    android:layout_height="83dip"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:focusable="true"
                    app:cardBackgroundColor="@color/accent19"
                    app:cardCornerRadius="@dimen/cardCornerRadius_small"
                    app:cardElevation="?cardelevation_tiny"
                    app:cardUseCompatPadding="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/accent_name_bg"
                        android:layout_gravity="bottom|start"
                        android:background="@drawable/gradient_card"
                        android:padding="@dimen/spacing_small">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
