                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/accent11"
                    android:layout_width="0dp"
                    android:layout_height="110dip"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#2A5D6A88"
