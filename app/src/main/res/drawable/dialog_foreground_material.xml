<?xml version="1.0" encoding="utf-8"?>
<inset android:inset="8.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <layer-list>
        <item>
            <shape android:shape="rectangle">
                <gradient
                    android:type="radial"
                    android:centerX="1.10"
                    android:centerY="0.0"
                    android:gradientRadius="200%p"
                    android:endColor="@null"
                    android:centerColor="@android:color/transparent"
                    android:startColor="#43648EF7" />
                <corners android:radius="16dp" />
            </shape>
        </item>
        <item>
            <shape android:tint="#73929FA8">
                <solid android:color="#30000000" />
                <corners android:radius="16dp" />
                <stroke android:width="2.6dp" android:color="?android:colorAccent" />
            </shape>
        </item>
    </layer-list>
</inset>