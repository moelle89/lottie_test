<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:enterFadeDuration="1600"
    android:exitFadeDuration="1600">
    <item android:state_checked="true">
        <shape android:shape="rectangle" android:tint="?colorControlActivated">
            <solid android:color="#64008EC2" />
            <corners android:radius="24.0dip" />
            <size android:width="21.0dip" android:height="21.0dip" />
            <stroke android:width="1.5dp" android:color="?colorControlActivated" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle" android:tint="?textcolor" >
            <solid android:color="#14FFFFFF" />
            <corners android:radius="24.0dip" />
            <size android:width="21.0dip" android:height="21.0dip" />
            <stroke android:width="1.5dp" android:color="#8CFFFFFF" />
        </shape>
    </item>
</selector>